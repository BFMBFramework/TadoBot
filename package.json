{
  "name": "tadobot",
  "version": "0.0.0",
  "description": "A bot for management of Tado devices through Telegram",
  "main": "./dist/main.js",
  "scripts": {
    "start": "node ./dist/main.js",
    "test": "./node_modules/.bin/mocha",
    "build": "gulp",
    "clean": "rm -rf ./dist",
    "rebuild": "npm run clean && npm run build",
    "gendocs": "typedoc --out ./docs ./src; touch ./docs/.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BFMBFramework/TadoBot.git"
  },
  "keywords": [
    "Tado",
    "bot",
    "Telegram"
  ],
  "author": "Ángel González",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BFMBFramework/TadoBot/issues"
  },
  "homepage": "https://github.com/BFMBFramework/TadoBot#readme",
  "dependencies": {
    "async": "^2.6.4",
    "jayson": "^2.1.0",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/async": "^2.0.50",
    "@types/winston": "^2.4.4",
    "gulp": "^4.0.0",
    "gulp-typescript": "^5.0.1",
    "mocha": "^5.2.0",
    "typescript": "^3.3.3333"
  }
}
